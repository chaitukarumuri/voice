<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>loginPage2</managed-bean-name>
		<managed-bean-class>presentation.LoginPage</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>log.xhtml</display-name>
		<from-view-id>/log.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>redo</from-outcome>
			<to-view-id>/log.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>log.xhtml</display-name>
		<from-view-id>/log.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/dashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dashboard.xhtml</display-name>
		<from-view-id>/dashboard.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>same</from-outcome>
			<to-view-id>/dashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dashboard.xhtml</display-name>
		<from-view-id>/dashboard.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/log.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>newsignup.xhtml</display-name>
		<from-view-id>/newsignup.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>signup</from-outcome>
			<to-view-id>/dashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>forgotpass.xhtml</display-name>
		<from-view-id>/forgotpass.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>validate</from-outcome>
			<to-view-id>/forgotpass1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>newpassword.xhtml</display-name>
		<from-view-id>/newpassword.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>changed</from-outcome>
			<to-view-id>/log.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>forgotpass1.xhtml</display-name>
		<from-view-id>/forgotpass1.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>true</from-outcome>
			<to-view-id>/newpassword.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>forgotpass1.xhtml</display-name>
		<from-view-id>/forgotpass1.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>false</from-outcome>
			<to-view-id>/forgotpass.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
